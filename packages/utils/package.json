        "@types/react-dom": "^18.0.4",
        "eslint-config-marshallku": "workspace:*",
        "react": "^18.1.0",
        "tsup": "^5.10.1",
        "typescript": "^4.5.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
