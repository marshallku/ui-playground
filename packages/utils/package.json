{
    "name": "@marshallku/utils",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
